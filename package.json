{
  "name": "hexo-site",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "css": "postcss ./themes/gemini-clean/source/css/main.css -o ./themes/gemini-clean/source/css/style.css",
    "watch:css": "postcss ./themes/gemini-clean/source/css/main.css -o ./themes/gemini-clean/source/css/style.css --watch",
    "build": "hexo clean && npm run css && hexo generate",
    "clean": "hexo clean",
    "deploy": "hexo deploy",
    "server": "hexo clean && npm run css && hexo server"
  },
  "hexo": {
    "version": "7.3.0"
  },
  "dependencies": {
    "@tailwindcss/postcss": "^4.1.12",
    "autoprefixer": "^10.4.21",
    "hexo": "^7.3.0",
    "hexo-deployer-git": "^4.0.0",
    "hexo-generator-archive": "^2.0.0",
    "hexo-generator-category": "^2.0.0",
    "hexo-generator-index": "^4.0.0",
    "hexo-generator-tag": "^2.0.0",
    "hexo-renderer-ejs": "^2.0.0",
    "hexo-renderer-marked": "^7.0.1",
    "hexo-server": "^3.0.0",
    "hexo-theme-landscape": "^1.0.0",
    "hexo-wordcount": "^6.0.1",
    "postcss": "^8.5.6",
    "postcss-cli": "^11.0.1",
    "tailwindcss": "^4.1.11"
  }
}
